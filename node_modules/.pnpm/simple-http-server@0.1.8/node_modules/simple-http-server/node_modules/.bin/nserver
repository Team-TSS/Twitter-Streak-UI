#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/akk017/Desktop/Twitter-Streak/node_modules/.pnpm/simple-http-server@0.1.8/node_modules/simple-http-server/bin/node_modules:/Users/akk017/Desktop/Twitter-Streak/node_modules/.pnpm/simple-http-server@0.1.8/node_modules/simple-http-server/node_modules:/Users/akk017/Desktop/Twitter-Streak/node_modules/.pnpm/simple-http-server@0.1.8/node_modules:/Users/akk017/Desktop/Twitter-Streak/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/akk017/Desktop/Twitter-Streak/node_modules/.pnpm/simple-http-server@0.1.8/node_modules/simple-http-server/bin/node_modules:/Users/akk017/Desktop/Twitter-Streak/node_modules/.pnpm/simple-http-server@0.1.8/node_modules/simple-http-server/node_modules:/Users/akk017/Desktop/Twitter-Streak/node_modules/.pnpm/simple-http-server@0.1.8/node_modules:/Users/akk017/Desktop/Twitter-Streak/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/server" "$@"
else
  exec node  "$basedir/../../bin/server" "$@"
fi
